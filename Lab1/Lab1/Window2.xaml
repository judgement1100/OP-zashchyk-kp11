        <Button x:Name="ButtonToMainWindow" Grid.Column="4" Grid.Row="2" FontSize="24" FontFamily="Algerian" Content="To main &#xD;&#xA;window" Background="#FFE5DF13" Cursor="Hand" Click="ButtonToMainWindow_Click"/>
        <Button x:Name="ExitButton" Grid.Column="4" Grid.Row="0" FontSize="24" FontFamily="Algerian" Content="Exit" Background="#FF13ACE5" Cursor="Hand" Click="Exit_Click"/>
        <Button x:Name="RestartButton" Grid.Column="4" Grid.Row="1" FontSize="24" FontFamily="Algerian" Content="Restart" Background="#FF1DDDB0" Cursor="Hand" Click="RestartButton_Click"/>
    </Grid>
</Window>
